{
  "name": "book-a-meal",
  "version": "1.0.0",
  "description": " ",
  "main": "app.js",
  "scripts": {
    "start:dev": "nodemon app.js --exec babel-node --",
    "start": "babel-node app.js",
    "test:dev": "  nyc --reporter=html --reporter=text mocha server/test --compilers js:babel-core/register --require babel-polyfill--timeout 50000ms --exit",
    "test": "npm run test:dev",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "report-coverage": "cat ./coverage/lcov.info | coveralls",
    "coveralls": "nyc --reporter=lcov --reporter=text-lcov npm test",
     "migrate": "sequelize db:migrate",
    "unmigrate": "sequelize db:migrate:undo:all",
    "build": "babel server -d build",
    "seed": "sequelize db:seed:all",
    "pretest": "NODE_ENV=test npm run migrate",
    "test": "NODE_ENV=test nyc mocha --compilers js:babel-core/register  test --exit",
    "posttest": "NODE_ENV=test npm run unmigrate",
    "heroku-postbuild":"npm run migrate"
  },
  "repository": {
    "type": "git"
  },
  "author": "olorunwa lawrence",
  "license": "ISC",
  "bugs": {},
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-preset-env": "^1.7.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "jsonwebtoken": "^8.5.0",
    "mocha": "^5.2.0",
    "moment": "^2.24.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "random-string": "^0.2.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "shortid": "^2.2.14",
    "validator": "^10.9.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.18.6",
    "nyc": "^13.1.0"
  }
}
